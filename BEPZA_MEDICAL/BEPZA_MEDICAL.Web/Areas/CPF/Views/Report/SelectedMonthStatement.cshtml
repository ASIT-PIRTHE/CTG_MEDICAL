@{
    ViewBag.Title = "SelectedReportViewer";
    Layout = "~/Areas/CPF/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm("GenerateReport", "Report", FormMethod.Post, new { id = "reportForm" }))
{
    <div class="row">
        <span class="label">
            Date From
            <label class="required-field">*</label>
        </span>
        <span class="field">
            <input type="date" name="FromDate" id="fromDate" class="datePicker required" style="width: 200px;">
        </span>
        <span class="label-right">
            To
            <label class="required-field">*</label>
        </span>
        <span class="field">
            <input type="date" name="ToDate" id="toDate" class="datePicker required" style="width: 200px;">
        </span>
    </div>

    <div class="GroupBox">
        <div class="form-group">
            <div class="text-center" style="text-align: center">
                <div class="">
                    <button class="btn btn-primary" type="submit" onclick="return fnValidate();" id="btnViewReport">
                        View Report
                    </button>
                </div>
            </div>
        </div>
    </div>
}