    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>WDeployAdmin</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="Default" Order="1" Enabled="False">
          <Destination Path="Data Source=PC88\SQL2012;Initial Catalog=VistaLOANDefault_v1;Persist Security Info=True;User ID=sa;Password=sa1234" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=PC88\SQL2012;Initial Catalog=VistaLOANDefault_v2;User ID=sa;Password=sa1234" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\Default_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=PC88\SQL2012; Initial Catalog=VistaLOANDefault_v2; User Id=sa; Password=sa1234;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
        <ObjectGroup Name="Bill" Order="2" Enabled="False">
