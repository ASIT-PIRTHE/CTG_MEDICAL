            <PreSource Path="Data Source=PC88\SQL2012;Initial Catalog=POMS_MPA;User ID=sa;Password=sa1234" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\POMS_MPA_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
