            <PreSource Path="Data Source=PC88\SQL2012;Initial Catalog=VistaLOANv2;User ID=sa;Password=sa1234" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\Bill_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
