  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Bill-Web.config Connection String">
      <ParameterValue>Data Source=PC88\SQL2012;Initial Catalog=VistaLOANv15;Persist Security Info=True;User ID=sa;Password=sa1234</ParameterValue>
